Listener(\"submit\", (evt) => {\r\n    errorText.classList.remove(\"error__none\")\r\n    errorText.classList.remove(\"correct__format\")\r\n    evt.preventDefault()\r\n    console.log(evt.target)\r\n    emailValidation()\r\n    /*\r\n    if(emailValidation){\r\n        alert(emailValidation())\r\n    }*/\r\n})"},"lineCount":null}},"error":null,"hash":"ad9fca8f4197db16be3f7577bd3db3fb","cacheData":{"env":{}}}